{
  "includes": [
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./types",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./lib"
  }
}