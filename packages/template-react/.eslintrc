{
  "extends": "plugin:react/recommended",
  "rules": {
    "@typescript-eslint/no-var-requires": 0,
    "react/react-in-jsx-scope": "off",
    "react/display-name": 0,
    "react/prop-types": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}